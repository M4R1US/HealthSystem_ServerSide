<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ListView?>


<BorderPane fx:id="UserModificationMainPane" minHeight="600" minWidth="600" stylesheets="/Stylesheets/DefaultStyle.css" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.UserModification">
    <top>
        <Label minWidth="600" minHeight="60" text="List of active users" alignment="CENTER"/>
    </top>
    <center>
        <ListView fx:id="activeUserList" minWidth="550" minHeight="400" maxWidth="550" maxHeight="500">

        </ListView>
    </center>
    <bottom>
        <HBox alignment="CENTER" minHeight="40" spacing="20">
            <Button fx:id="DisableByLoginButton" minWidth="100" text="Disable by login"/>
            <Button fx:id="DisableByIpButton" minWidth="100" text="Disable by IP"/>
        </HBox>
    </bottom>
</BorderPane>
